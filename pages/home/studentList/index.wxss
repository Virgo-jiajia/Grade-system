.stu-list {
  padding-bottom: 40rpx;
}
.stu-list .list-box {
  width: 686rpx;
  margin: 0 auto;
  background: #FFFFFF;
  border-radius: 12rpx;
  margin-bottom: 26rpx;
}
.stu-list .list-box .list-item {
  padding: 30rpx 0rpx 30rpx 17rpx;
  font-size: 32rpx;
  color: #333333;
  display: flex;
  align-items: center;
}
.stu-list .list-box .list-item image {
  width: 28rpx;
  height: 35rpx;
  margin-right: 14rpx;
}
.stu-list .list-box .list-item text {
  width: 400rpx;
}
.stu-list .list-box .list-item button {
  width: 144rpx;
  height: 62rpx;
  color: #ffffff;
  background: #CC2A2A;
  border-radius: 70rpx;
  font-size: 26rpx;
  font-weight: 400;
  padding: 12rpx 20rpx;
  margin-right: 26rpx;
}
